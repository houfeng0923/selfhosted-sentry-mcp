{
  "name": "sentry-mcp",
  "version": "0.1.0",
  "description": "Model context protocol server for self hosted sentry instances, 支持自动读取 ~/.sentryclirc 配置。",
  "private": true,
  "type": "module",
  "bin": {
    "sentry-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "@types/ini": "^4.1.1",
    "axios": "^1.8.4",
    "ini": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}